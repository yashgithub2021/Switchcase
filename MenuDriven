#include<stdio.h>
main()
{
	int a;
	printf("Enter your choice :");
	scanf("%d",&a);
	
	switch(a)
	{
		    case 1:
			printf("Food Item - Sandwich \nPrice - 90/-");
			break;
		    case 2:
			printf("Food Item - Cold Coffee \nPrice - 50/-");
			break;
			case 3:
			printf("Food Item - Fries \nPrice - 70/-");
			break;
			case 4:
			printf("Food Item - Pizza \nPrice - 150/-");
			break;
			default:
			printf("Invalid Choice...");	
	}
}
